<div class="title-container text-light">
	<h1>Login</h1>
</div>

<%= form_tag '/login' do %>
   <%= label_tag :username%>
   <%= text_field_tag :username %>
   <%= label_tag :password%>
   <%= password_field_tag :password%>
   <%= submit_tag "Login"%>
<%end%>

<%= link_to 'Register', new_user_path %>