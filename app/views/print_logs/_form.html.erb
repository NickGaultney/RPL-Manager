<%= form_with(model: print_log, local: true) do |form| %>
  <% if print_log.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(print_log.errors.count, "error") %> prohibited this print_log from being saved:</h2>

      <ul>
      <% print_log.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group">
    <%= form.label :printer_name, class:"text-light" %>
    <%= form.text_field :printer_name, id: :print_log_printer_name, class:"form-control" %>
  </div>

  <div class="form-group">
    <%= form.label :file_name, class:"text-light" %>
    <%= form.text_field :file_name, id: :print_log_file_name, class:"form-control" %>
  </div>

  <div class="form-group">
    <%= form.label :filament_weight, class:"text-light" %>
    <%= form.text_field :filament_weight, id: :print_log_filament_weight, class:"form-control" %>
  </div>

  <div class="form-group">
    <%= form.label :print_time, class:"text-light" %>
    <%= form.text_field :print_time, id: :print_log_print_time, class:"form-control" %>
  </div>

  <div class="form-group">
    <%= form.label :cost, class:"text-light" %>
    <%= form.text_field :cost, id: :print_log_cost, class:"form-control" %>
  </div>

  <div class="form-group">
    <%= form.label :status, class:"text-light" %>
    <%= form.text_field :status, id: :print_log_status, class:"form-control" %>
  </div>

  <div class="actions">
    <%= form.submit class:"btn btn-primary" %>
  </div>
<% end %>
